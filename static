class Counter {
    static int count = 0;
    static void incrementCount() {
        count++;
    }
    static int getCount() {
        return count;
    }
}

public class Main {
    public static void main(String[] args) {
        Counter.incrementCount();
        Counter.incrementCount();
        Counter.incrementCount();
        System.out.println("Current count: " + Counter.getCount());
    }
}
